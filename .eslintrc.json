{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-multiple-empty-lines":"off",
        "no-trailing-spaces":"off",
        "linebreak-style":"off",
        "indent":"off",
        "eol-last":"off",
        "no-underscore-dangle" : "off"
    }
}
